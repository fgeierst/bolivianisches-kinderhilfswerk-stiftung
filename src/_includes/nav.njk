<expanding-nav breakpoint="800">
  <span slot="button" class="menu-button">Menu</span>

  {% set navPages = collections.all | eleventyNavigation %}
  <ul class="nav">
  {%- for entry in navPages %}
    <li class="nav__item">
      <a class="nav__link"
        href="{{ entry.url | url }}"
        {% if entry.url == page.url %}aria-current="page"{% endif %}
      >
        {{ entry.title }}
      </a>
    </li>
  {%- endfor %}
  </ul>
</expanding-nav>

